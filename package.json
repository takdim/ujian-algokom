{
  "name": "algokom-demo",
  "version": "1.0.0",
  "description": "ALGOKOM Demo - Fibonacci & Bilinear Interpolation with C backend",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "compile": "gcc-15 -fopenmp -DUSE_OPENMP -o fib_omp_json fibonacci_json.c",
    "setup": "npm install && npm run compile",
    "test": "curl http://localhost:3000/api/fibonacci/35"
  },
  "keywords": [
    "fibonacci",
    "openmp",
    "bilinear-interpolation",
    "parallel-computing"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  }
}
